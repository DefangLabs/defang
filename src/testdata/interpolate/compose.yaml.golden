name: interpolate
services:
  interpolate:
    environment:
      BRACED: interpolate
      DB: postgres://user:${POSTGRES_PASSWORD}@db:5432/db
      NAME: interpolate
      NODE_ENV: ${NODE_ENV}
      NOP: abc$def
      NOP_BRACED: abc${def}
      PORT: "8080"
      interpolate: value
    image: alpine
    networks:
      default: null
networks:
  default:
    name: interpolate_default
