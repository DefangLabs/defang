name: railpack
services:
  railpack-long:
    build:
      context: .
      dockerfile: Dockerfile
    command:
      - exec
      - node
      - index.js
    networks:
      default: null
  railpack-short:
    build:
      context: .
      dockerfile: Dockerfile
    command:
      - npm
      - start
    networks:
      default: null
  railpackwithdockerfile:
    build:
      context: .
      dockerfile: Dockerfile
    command:
      - something
      - with space
      - ${PORT}
    networks:
      default: null
networks:
  default:
    name: railpack_default
