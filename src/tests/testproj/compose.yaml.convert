[
  {
    "name": "dfnx",
    "platform": 2,
    "internal": true,
    "deploy": {
      "replicas": 1,
      "resources": {
        "reservations": {
          "memory": 256,
          "cpus": 0.25
        }
      }
    },
    "ports": [
      {
        "target": 80,
        "mode": 1
      },
      {
        "target": 1234,
        "mode": 1
      }
    ],
    "environment": {
      "DOTENV": "enabled",
      "DOT_ENV_INTERPOLATION": "enabled",
      "FOO": "bar"
    },
    "build": {
      "context": ".",
      "dockerfile": "Dockerfile",
      "args": {
        "DNS": "dfnx"
      },
      "target": "testproj"
    },
    "secrets": [
      {
        "source": "dummy"
      }
    ],
    "healthcheck": {
      "test": [
        "CMD",
        "curl",
        "-f",
        "http://localhost/"
      ]
    },
    "networks": 1
  }
]