name: postgres
services:
  x:
    image: postgres
    networks:
      default: null
    x-defang-postgres:
      maintenance:
        day-of-week: Thursday
        duration: 1
        start-time: "23:00"
      retention:
        number-of-days-to-keep: 30
        restore-on-startup: true
        save-on-deprovisioning: true
  "y":
    image: example
    networks:
      default: null
    x-defang-postgres: null
  z:
    image: postgres
    networks:
      default: null
networks:
  default:
    name: postgres_default
