name: postgres
services:
  x:
    image: postgres
    networks:
      default: null
    x-defang-postgres:
      allow-downtime: true
      retention:
        final-snapshot-name: final-snapshot
        retention-period: 30d
        snapshot-to-load-on-startup: old-snapshot
  "y":
    image: example
    networks:
      default: null
    x-defang-postgres: null
  z:
    image: postgres
    networks:
      default: null
networks:
  default:
    name: postgres_default
