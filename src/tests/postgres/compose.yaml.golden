name: postgres
services:
  x:
    image: postgres
    networks:
      default: null
    x-defang-postgres:
      maintenance-window: Mon:00:00-Mon:01:00
      retention:
        backup-window: 23:30-00:30
        final-snapshot-name: final-snapshot
        retention-period: 30
        snapshot-to-load-on-startup: old-snapshot
  "y":
    image: example
    networks:
      default: null
    x-defang-postgres: null
  z:
    image: postgres
    networks:
      default: null
networks:
  default:
    name: postgres_default
