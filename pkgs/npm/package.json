{
  "name": "defang",
  "version": "<PLACEHOLDER>",
  "author": "Defang Software Labs Inc.",
  "description": "CLI to take your app from Docker Compose to a secure and scalable deployment on your favorite cloud in minutes",
  "license": "MIT",
  "bin": {
    "defang": "bin/cli.js"
  },
  "keywords": [],
  "homepage": "https://defang.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DefangLabs/defang.git"
  },
  "bugs": {
    "url": "https://github.com/DefangLabs/defang/issues"
  },
  "mocha": {
    "require": [
      "ts-node/register"
    ],
    "extension": [
      "ts",
      "js"
    ],
    "spec": [
      "test/**/*.spec.ts"
    ],
    "recursive": true
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "chmod +x bin/cli.js",
    "test": "TS_NODE_PROJECT='./tsconfig-test.json' mocha -r ts-node/register --loader=ts-node/esm -trace-warnings --no-warnings=ExperimentalWarning test/*.spec.ts"
  },
  "dependencies": {
    "adm-zip": "^0.5.14",
    "axios": "^1.7.2",
    "babel-register-esm": "^1.2.5",
    "https": "^1.0.0",
    "os": "^0.1.2",
    "tar": "^7.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@types/adm-zip": "^0.5.5",
    "@types/chai": "^4.3.16",
    "@types/chai-as-promised": "^7.1.8",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.12.7",
    "@types/sinon": "^17.0.3",
    "@types/source-map-support": "^0.5.10",
    "@types/tar": "^6.1.13",
    "chai": "^5.1.1",
    "chai-as-promised": "^8.0.0",
    "cross-env": "^7.0.3",
    "mocha": "^10.6.0",
    "sinon": "^18.0.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "main": "./bin/cli.js"
}
