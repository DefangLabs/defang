{
  "name": "defang",
  "version": "0.5.32",
  "author": "Defang Software Labs Inc.",
  "description": "CLI for the Defang Opinionated Platform",
  "license": "MIT",
  "type": "module",
  "bin": {
    "defang": "./bin/cli.js"
  },
  "keywords": [],
  "homepage": "https://github.com/DefangLabs/defang#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DefangLabs/defang.git"
  },
  "bugs": {
    "url": "https://github.com/DefangLabs/defang/issues"
  },
  "compilerOptions": {
    "target": "ESNext",
    "module": "module",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "mocha": {
    "require": [
      "ts-node/register"
    ],
    "extension": [
      "ts",
      "js"
    ],
    "spec": "./test/**/*.ts",
    "recursive": true
  },
  "scripts": {
    "build": "tsc",
    "test": "cross-env NODE_OPTIONS=\"-r ts-node/register --loader=ts-node/esm --trace-warnings --no-warnings=ExperimentalWarning\" TS_NODE_COMPILER_OPTIONS='{\"module\": \"ESNext\" }' mocha './test/**/*.ts'"
  },
  "dependencies": {
    "adm-zip": "^0.5.14",
    "axios": "^1.7.2",
    "babel-register-esm": "^1.2.5",
    "https": "^1.0.0",
    "os": "^0.1.2",
    "tar": "^7.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@types/adm-zip": "^0.5.5",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.3.16",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.12.7",
    "@types/sinon": "^17.0.3",
    "@types/source-map-support": "^0.5.10",
    "@types/tar": "^6.1.13",
    "chai": "^5.1.1",
    "cross-env": "^7.0.3",
    "mocha": "^10.6.0",
    "nyc": "^17.0.0",
    "sinon": "^18.0.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "main": "./bin/cli.js"
}
