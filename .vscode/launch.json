    "version": "0.2.0",
    "configurations": [
       {
            "name": "Launch flask compose up",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "./src/cmd/cli",
            "console": "integratedTerminal",
            "cwd": "~/Documents/Github/samples/samples/langchain",
            "args": [
                "compose",
                "up",
            ]
        },
        {
            "name": "Launch vuejs compose up",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "./src/cmd/cli",
            "console": "integratedTerminal",
            "cwd": "~/Documents/Github/samples/samples/vuejs",
            "args": [
                "compose",
                "up",
                "--verbose",
                "--debug"
            ]
        },
        {
            "name": "Launch generate",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "./src/cmd/cli",
            "console": "integratedTerminal",
            "cwd": "~/Documents/Void",
            "args": [
                "generate",
                "--verbose",
                "--debug"
            ]
        },
        {
            "name": "Launch provider cd ls",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "./src/cmd/cli",
            "console": "integratedTerminal",
            "cwd": "~/Documents/Github/samples/samples/vuejs",
            "args": [
                "cd",
                "ls",
                "--provider=aws"
            ]
        },

        {
            "name": "Launch region provider compose up",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "./src/cmd/cli",
            "console": "integratedTerminal",
            "cwd": "~/Documents/Github/samples/samples/vuejs",
            "env": {"AWS_REGION":"us-west-2"},
            "args": [
                "up",
                "--provider=aws"
            ]
        },
        {
            "name": "Launch help",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "./src/cmd/cli",
            "console": "integratedTerminal",
            "cwd": "~/Documents/Github/samples/samples/flask",
            "args": [
                "restart"
            ]
        },
    ]
}
