      "args": [
        "-C",
        "/Users/jordan/wk/samples/samples/html-css-js",
        "logs",
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "GCP CLI",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/src/cmd/cli",
      "env": {
        "DEFANG_PROVIDER": "gcp",
        "GCP_PROJECT_ID": "jordan-project-463223"
      },
      "args": [
        "-C",
        "/Users/jordan/wk/samples/samples/html-css-js",
        "logs",
      ],
      "console": "integratedTerminal"
